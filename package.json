{
  "name": "@boltfive505/paymongo-node-js",
  "version": "1.3.4",
  "description": "My personal paymongo nodejs library (converted to commonjs)",
  "main": "lib/index.js",
  "typings": "lib/index.js",
  "types": "lib",
  "scripts": {
    "test": "jest --verbose false --config jestconfig.json",
    "prebuild": "rimraf lib && tsc --emitDeclarationOnly",
    "build": "babel src -d lib --extensions \".js,.ts,.tsx,.jsx\" --copy-files --source-maps inline",
    "publish": "npm publish --access=public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boltfive505/paymongo-node-js.git"
  },
  "keywords": [
    "paymongo",
    "api",
    "node",
    "nodejs",
    "commonjs",
    "typescript"
  ],
  "author": "boltfive505",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boltfive505/paymongo-node-js/issues"
  },
  "homepage": "https://github.com/boltfive505/paymongo-node-js#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/base-64": "^1.0.0",
    "axios": "^0.23.0",
    "base-64": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.15.0",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.3.1",
    "babel-plugin-module-resolver": "^4.1.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.4"
  }
}
