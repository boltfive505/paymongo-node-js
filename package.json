{
  "name": "paymongo-node-js",
  "version": "1.0.0",
  "description": "My personal paymongo nodejs library (mainly for React Native Expo)",
  "main": "lib/index.js",
  "typings": "lib/index.js",
  "types": "lib",
  "scripts": {
    "test": "jest --verbose false --config jestconfig.json",
    "prebuild": "rimraf dist && tsc && npm run test",
    "build": "rollup -c rollup.config.js",
    "build-only": "rollup -c rollup.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/boltfive505/paymongo-node-js.git"
  },
  "keywords": [
    "paymongo",
    "api",
    "node",
    "nodejs",
    "react",
    "react native",
    "expo"
  ],
  "author": "boltfive505",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/boltfive505/paymongo-node-js/issues"
  },
  "homepage": "https://github.com/boltfive505/paymongo-node-js#readme",
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/base-64": "^1.0.0",
    "axios": "^0.23.0",
    "base-64": "^1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@open-wc/building-rollup": "^2.0.1",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/jest": "^27.0.2",
    "babel-jest": "^27.3.1",
    "deepmerge": "^4.2.2",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-uglify": "^6.0.4",
    "ts-jest": "^27.0.7",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.4.4"
  }
